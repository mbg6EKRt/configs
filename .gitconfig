[user]
	useConfigOnly = true
[alias]
	st = status
	a = add
	ci = commit
	ps = push
	pl = pull
	br = branch
	co = checkout
	com = checkout master
	cb = checkout -b
	rb = rebase
	rbm = rebase master
	lg = log --graph --all --pretty='%Cred%h%Creset %Cgreen%ad%Creset -%C(auto)%d%Creset %s' --date=human
	lga = log --graph --all --pretty='%Cred%h%Creset %Cgreen%ad %C(bold blue)%an%Creset - %C(auto)%d%Creset %s' --date=human
