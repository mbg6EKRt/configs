[core]
	editor = nano
[user]
	useConfigOnly = true
[alias]
	s = status
	d = diff
	dc = diff --cached
	a = add
	c = commit
	ps = push
	pl = pull
	br = branch
	co = checkout
	com = checkout master
	cb = checkout -b
	rb = rebase
	rbm = rebase master
	l = log --graph --all --pretty='%Cred%h%Creset %Cgreen%ad%Creset -%C(auto)%d%Creset %s' --date=human
	la = log --graph --all --pretty='%Cred%h%Creset %Cgreen%ad %C(bold blue)%an%Creset - %C(auto)%d%Creset %s' --date=human
